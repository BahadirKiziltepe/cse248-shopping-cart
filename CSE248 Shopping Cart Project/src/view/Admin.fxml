<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AdminController">
   <children>
      <AnchorPane prefHeight="480.0" prefWidth="720.0">
         <children>
            <ImageView fitHeight="458.0" fitWidth="720.0" layoutY="23.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/Admin.png" />
               </image>
            </ImageView>
            <Button fx:id="accountsBtn" layoutX="500.0" layoutY="61.0" mnemonicParsing="false" onAction="#showAccounts" prefHeight="25.0" prefWidth="100.0" text="Accounts">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="itemsBtn" layoutX="500.0" layoutY="131.0" mnemonicParsing="false" onAction="#showItems" prefHeight="25.0" prefWidth="100.0" text="Items">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="ordersBtn" layoutX="500.0" layoutY="201.0" mnemonicParsing="false" onAction="#showOrders" prefHeight="25.0" prefWidth="100.0" text="Orders">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="wareHouseBtn" layoutX="500.0" layoutY="271.0" mnemonicParsing="false" onAction="#showWareHouse" prefHeight="25.0" prefWidth="100.0" text="WareHouse">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="accountBtn" layoutX="500.0" layoutY="341.0" mnemonicParsing="false" onAction="#showAccount" prefHeight="25.0" prefWidth="100.0" text="Account">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="logoutBtn" layoutX="500.0" layoutY="411.0" mnemonicParsing="false" onAction="#logout" prefHeight="25.0" prefWidth="100.0" text="Logout">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>

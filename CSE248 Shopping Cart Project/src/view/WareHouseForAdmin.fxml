<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.WareHouseControllerForAdmin">
   <children>
      <AnchorPane prefHeight="480.0" prefWidth="720.0">
         <children>
            <ImageView fitHeight="295.0" fitWidth="720.0" layoutX="1.0" layoutY="184.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/warehouseBG.png" />
               </image>
            </ImageView>
            <Button fx:id="cancelBtn" layoutX="317.0" layoutY="146.0" mnemonicParsing="false" onAction="#cancelOrder" text="Cancel Order">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <ListView fx:id="orderList" layoutX="14.0" layoutY="14.0" prefHeight="393.0" prefWidth="200.0" />
            <Button fx:id="viewBtn" layoutX="222.0" layoutY="146.0" mnemonicParsing="false" onAction="#viewOrder" text="View Order">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="mainMenu" layoutX="611.0" layoutY="146.0" mnemonicParsing="false" onAction="#mainMenu" text="Main Menu">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="480.0" prefWidth="720.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AccountsControllerForAdmin">
   <children>
      <AnchorPane prefHeight="480.0" prefWidth="720.0">
         <children>
            <ListView fx:id="accountViewer" layoutX="406.0" layoutY="15.0" onMouseClicked="#getSelected" prefHeight="450.0" prefWidth="300.0" />
            <TextField fx:id="searchBar" layoutX="93.0" layoutY="51.0" prefHeight="30.0" prefWidth="200.0">
               <font>
                  <Font size="15.0" />
               </font>
            </TextField>
            <Button fx:id="searchBtn" layoutX="161.0" layoutY="111.0" mnemonicParsing="false" onAction="#searchUsers" text="Search">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="selectAccount" layoutX="245.0" layoutY="398.0" mnemonicParsing="false" onAction="#viewAccount" text="Select Account">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Text fx:id="userName" layoutX="77.0" layoutY="218.0" strokeType="OUTSIDE" strokeWidth="0.0" text="username">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Text fx:id="password" layoutX="77.0" layoutY="246.0" strokeType="OUTSIDE" strokeWidth="0.0" text="password">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Text fx:id="email" layoutX="77.0" layoutY="274.0" strokeType="OUTSIDE" strokeWidth="0.0" text="email">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Text fx:id="firstName" layoutX="77.0" layoutY="303.0" strokeType="OUTSIDE" strokeWidth="0.0" text="first name">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Text fx:id="middleName" layoutX="77.0" layoutY="332.0" strokeType="OUTSIDE" strokeWidth="0.0" text="middle name">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Text fx:id="lastName" layoutX="77.0" layoutY="361.0" strokeType="OUTSIDE" strokeWidth="0.0" text="last name">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Text fx:id="street" layoutX="272.0" layoutY="236.0" strokeType="OUTSIDE" strokeWidth="0.0" text="street">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Text fx:id="state" layoutX="272.0" layoutY="264.0" strokeType="OUTSIDE" strokeWidth="0.0" text="state">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Text fx:id="city" layoutX="272.0" layoutY="292.0" strokeType="OUTSIDE" strokeWidth="0.0" text="city">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Text fx:id="zipcode" layoutX="272.0" layoutY="321.0" strokeType="OUTSIDE" strokeWidth="0.0" text="zipcode">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Text fx:id="country" layoutX="272.0" layoutY="350.0" strokeType="OUTSIDE" strokeWidth="0.0" text="country">
               <font>
                  <Font size="15.0" />
               </font>
            </Text>
            <Button fx:id="backBtn" layoutX="68.0" layoutY="417.0" mnemonicParsing="false" onAction="#goBack" text="Main Menu">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="deleteBtn" layoutX="243.0" layoutY="436.0" mnemonicParsing="false" onAction="#deleteAccount" text="Delete Account">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>

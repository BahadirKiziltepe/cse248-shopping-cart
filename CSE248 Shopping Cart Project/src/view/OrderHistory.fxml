<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<VBox prefHeight="480.0" prefWidth="720.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="controllers.OrderHistoryController">
   <children>
      <AnchorPane prefHeight="480.0" prefWidth="720.0">
         <children>
            <ListView fx:id="orderList" layoutX="14.0" layoutY="44.0" prefHeight="393.0" prefWidth="200.0" />
            <Button fx:id="cancelBtn" layoutX="331.0" layoutY="406.0" mnemonicParsing="false" onAction="#cancelOrder" text="Cancel Order">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <Button fx:id="viewBtn" layoutX="226.0" layoutY="406.0" mnemonicParsing="false" onAction="#viewOrder" text="View Order">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
            <ImageView fitHeight="314.0" fitWidth="345.0" layoutX="331.0" layoutY="44.0" pickOnBounds="true" preserveRatio="true" rotate="10.0">
               <image>
                  <Image url="@../image/orderHistory.png" />
               </image>
            </ImageView>
            <Button fx:id="mainMenu" layoutX="610.0" layoutY="406.0" mnemonicParsing="false" onAction="#mainMenu" text="Main Menu">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</VBox>
